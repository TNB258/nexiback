      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "terms": {
      "type": "text"
    },
    "payment": {
      "type": "string"
    },
    "stock": {
      "type": "integer"
    },
    "size": {
      "type": "string"
    },
    "notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::notification.notification",
